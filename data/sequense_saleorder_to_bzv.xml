<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequences for sale.order -->
    <record id="sale.seq_sale_order" model="ir.sequence">
        <field name="name">Sales Order</field>
        <field name="code">sale.order</field>
        <field name="prefix">BZV-%(range_year)s%(month)s%(day)s</field>
        <field name="padding">5</field>
    </record>
    <!-- Sequences for internal transfers zu QM -->
    <record id="stock.seq_wh_in" model="ir.sequence">
        <field name="name">Adlershof Sequenz QM</field>
        <field name="prefix">WH/QM/</field>
        <field name="padding">5</field>
    </record>
</odoo>
